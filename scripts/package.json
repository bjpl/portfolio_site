{
  "name": "portfolio-test-scripts",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Test scripts for portfolio site infrastructure",
  "main": "test-supabase.js",
  "scripts": {
    "test:supabase": "node test-supabase.js",
    "test:admin-panel": "node test-admin-panel.js",
    "test:migrations": "node test-migrations.js",
    "test:all": "node test-supabase.js && node test-admin-panel.js && node test-migrations.js",
    "migrate": "node execute-migrations.js execute",
    "migrate:status": "node execute-migrations.js status",
    "migrate:rollback": "node execute-migrations.js rollback",
    "migrate:validate": "node execute-migrations.js validate",
    "migrate:content": "node migrate-content.js",
    "migrate:content:dry-run": "node migrate-content.js --dry-run",
    "migrate:content:blog": "node migrate-content.js --content-type=blog",
    "migrate:content:poetry": "node migrate-content.js --content-type=poetry",
    "migrate:content:projects": "node migrate-content.js --content-type=projects"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "chalk": "^5.6.0",
    "dotenv": "^17.2.1",
    "gray-matter": "^4.0.3",
    "ora": "^8.2.0",
    "pg": "^8.16.3"
  },
  "keywords": [
    "testing",
    "supabase",
    "portfolio",
    "infrastructure"
  ],
  "author": "Portfolio Site",
  "license": "MIT"
}
