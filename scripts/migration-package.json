{
  "name": "hugo-supabase-migration-scripts",
  "version": "1.0.0",
  "description": "Hugo to Supabase content migration utilities",
  "main": "hugo-content-parser.js",
  "scripts": {
    "migrate": "node hugo-content-parser.js",
    "migrate-media": "node hugo-media-migrator.js",
    "migrate-all": "npm run setup-schema && npm run migrate && npm run migrate-media",
    "setup-schema": "psql $SUPABASE_DB_URL -f hugo-schema-setup.sql",
    "test-migration": "node --trace-warnings hugo-content-parser.js --dry-run",
    "validate": "node validate-migration.js",
    "rollback": "node rollback-migration.js",
    "stats": "node migration-stats.js"
  },
  "keywords": [
    "hugo",
    "supabase",
    "migration",
    "content-management",
    "static-site-generator"
  ],
  "author": "Brandon JP Lambert",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "gray-matter": "^4.0.3",
    "glob": "^10.3.10",
    "mime-types": "^2.1.35",
    "dotenv": "^16.4.5",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonjplambert/portfolio_site.git"
  },
  "bugs": {
    "url": "https://github.com/brandonjplambert/portfolio_site/issues"
  },
  "homepage": "https://github.com/brandonjplambert/portfolio_site#readme"
}